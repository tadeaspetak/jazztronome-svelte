{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"compilerOptions": {
		"resolveJsonModule": true,
		"importsNotUsedAsValues": "preserve",
		"lib": [
			"DOM",
			"ES2019",
			"WebWorker"
		]
	},
	"include": [
		"src/**/*",
		"src/node_modules/**/*"
	],
	"exclude": [
		"node_modules/*",
		"__sapper__/*",
		"static/*"
	]
}